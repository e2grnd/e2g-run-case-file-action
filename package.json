    "immutable": "^4.3.0",
    "lodash": "^4.17.21",
    "lodash.find": "^4.6.0",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@typescript-eslint/parser": "^5.59.2",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.44.0",
    "eslint-plugin-github": "^4.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
